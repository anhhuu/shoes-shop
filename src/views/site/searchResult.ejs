<!-- top Products -->
<div class="ads-grid_shop">
    <div class="shop_inner_inf">
        <!-- product right -->
        <div class="left-ads-display col-md-12">
            <div class="row">
                <h3><%= searchResultTitle %> </h3>
            </div>
            <div class="row">
                <div class="pagination">
                    <% if(products.length) {%>
                    <% if(keyword) { %>
                    <a href="/search?page=1&keyword=<%= keyword %>">&laquo;</a>
                    <%  } else { %>
                    <a href="/search?page=1&keyword=<%= keyword %>">&laquo;</a>
                    <% } %>

                    <%for (let j = 0; j < numOfPage; j++) {%>
                    <%if(j+1 == currentPage) {%>
                    <% if(keyword) { %>
                    <a href="/search?page=<%= j+1 %>&keyword=<%= keyword %> " class="active">
                        <%= j+1 %>
                    </a>
                    <%  } else { %>
                    <a href="/search?page=<%= j+1 %>" class="active">
                        <%= j+1 %>
                    </a>
                    <% } } else {%>
                    <% if(keyword) { %>
                    <a href="/search?page=<%= j+1 %>&keyword=<%= keyword %>">
                        <%= j+1 %>
                    </a>
                    <%  } else { %>
                    <a href="/search?page=<%= j+1 %>">
                        <%= j+1 %>
                    </a>
                    <% } %>
                    <% } %>
                    <% } %>

                    <% if(keyword) { %>
                    <a href="/search?page=<%= numOfPage %>&keyword=<%= keyword %>">&raquo;</a>
                    <%  } else { %>
                    <a href="/search?page=<%= numOfPage %>&keyword=<%= keyword %>">&raquo;</a>
                    <% } %>
                    <% } %>
                </div>
            </div>
            <div class="wrapper_top_shop">
                <!-- <div class="col-md-6 shop_left">
                    <img src="images/banner3.jpg" alt="">
                    <h6>40% off</h6>
                </div>
                <div class="col-md-6 shop_right">
                    <img src="images/banner2.jpg" alt="">
                    <h6>50% off</h6>
                </div>
                <div class="clearfix"></div> -->
                <!-- product-sec1 -->
                <div class="product-sec1 row-custom">
                    <% for(let i = 0; i < products.length; i++) { %>
                    <div class="col-md-3 product-men">
                        <div class="product-shoe-info shoe">
                            <div class="men-pro-item">
                                <div class="men-thumb-item">
                                    <img src="<%= products[i].image_show_url %>" alt="">
                                    <div class="men-cart-pro">
                                        <div class="inner-men-cart-pro">
                                            <a href="products/<%= products[i].product_url %>"
                                                class="link-product-add-cart">Quick
                                                View</a>
                                        </div>
                                    </div>

                                </div>
                                <div class="item-info-product">
                                    <h5>
                                        <br>
                                        <a href="products/<%= products[i].product_url %>">
                                            <%= products[i].name %>
                                        </a>
                                    </h5>
                                    <div class="info-product-price">
                                        <div class="grid_meta">
                                            <div class="product_price">
                                                <div class="grid-price ">
                                                    <span class="money "><%= products[i].price.string_price %></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <% } %>
                </div>
                <div class="clearfix"></div>

            </div>
        </div>
        <div class="clearfix"></div>
    </div>
</div>
<!-- //top products -->

