<div id="multi-item-example" class="carousel slide carousel-multi-item product-card " data-ride="carousel" data-interval="2000">

    <!--Controls-->
    <div class="controls-top">
        <a class="btn-floating float-left" href="#multi-item-example" data-slide="prev"><i
                    class="fa fa-chevron-left"></i></a>
        <a class="btn-floating float-right" href="#multi-item-example" data-slide="next"><i
                    class="fa fa-chevron-right"></i></a>
    </div>
    <!--/.Controls-->

    <!--Slides-->
    <div class="carousel-inner d-inline " role="listbox" id="carousel-related">
        <% for(let i = 0; i < 2;i++){ %>
            <% if(i === 0 ) { %>
        <div class="carousel-item active">
            <% }else{ %>
            <div class="carousel-item">
                <% } %>
                <% for(let j = 0;j < 4 && j< options.brands.length;j++){ %>
                    <img src="<%= options.brands[j].image_url %>" alt="">
                <% } %>
            </div>
            <% } %>

        </div>
        <!--/.Slides-->
    </div>

</div>