        <!-- product right -->
        <div class="left-ads-display col-md-9">
            <div class="row">
                <div class="pagination">
                    <% if(brandChecked) { %>
                    <a href="/products?page=1&brand=<%= brandChecked %>">&laquo;</a>
                    <%  } else { %>
                    <a href="/products?page=1&brand=<%= brandChecked %>">&laquo;</a>
                    <% } %>

                    <%for (let j = 0; j < numOfPage; j++) {%>
                    <%if(j+1 == currentPage) {%>
                    <% if(brandChecked) { %>
                    <a href="/products?page=<%= j+1 %>&brand=<%= brandChecked %> " class="active">
                        <%= j+1 %>
                    </a>
                    <%  } else { %>
                    <a href="/products?page=<%= j+1 %>" class="active">
                        <%= j+1 %>
                    </a>
                    <% } } else {%>
                    <% if(brandChecked) { %>
                    <a href="/products?page=<%= j+1 %>&brand=<%= brandChecked %>">
                        <%= j+1 %>
                    </a>
                    <%  } else { %>
                    <a href="/products?page=<%= j+1 %>">
                        <%= j+1 %>
                    </a>
                    <% } %>
                    <% } %>
                    <% } %>

                    <% if(brandChecked) { %>
                    <a href="/products?page=<%= numOfPage %>&brand=<%= brandChecked %>">&raquo;</a>
                    <%  } else { %>
                    <a href="/products?page=<%= numOfPage %>&brand=<%= brandChecked %>">&raquo;</a>
                    <% } %>
                </div>
            </div>
