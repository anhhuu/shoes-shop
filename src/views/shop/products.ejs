                <!-- product-sec1 -->
                <div class="product-sec1 row-custom">
                    <% for(let i = 0; i < products.length; i++) { %>
                        <div class="col-md-3 product-men">
                            <div class="product-shoe-info shoe">
                                <div class="men-pro-item">
                                    <div class="men-thumb-item">
                                        <img src="<%= products[i].image_show_url %>" alt="">
                                        <div class="men-cart-pro">
                                            <div class="inner-men-cart-pro">
                                                <a href="products/<%= products[i].product_url %>"
                                                   class="link-product-add-cart">Quick
                                                    View</a>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="item-info-product">
                                        <h5>
                                            <br>
                                            <a href="products/<%= products[i].product_url %>">
                                                <%= products[i].name %>
                                            </a>
                                        </h5>
                                        <div class="info-product-price">
                                            <div class="grid_meta">
                                                <div class="product_price">
                                                    <div class="grid-price ">
                                                    <span class="money ">
                                                        <%= products[i].price.string_price %>
                                                    </span>
                                                    </div>

                                                </div>

                                            </div>
                                            <div class="shoe single-item ">
                                                <form  method="post" >
                                                    <input type="hidden" name="cmd-<%=products[i]._id%>" value="_cart">
                                                    <input type="hidden" name="add-<%=products[i]._id%>" value="1">
                                                    <input type="hidden" name="shoe_img_url-<%=products[i]._id%>" value="<%=products[i].image_show_url%>">
                                                    <input type="hidden" name="shoe_id-<%=products[i]._id%>" value="<%=products[i]._id%>">
                                                    <input type="hidden" name="shoe_url-<%=products[i]._id%>" value="<%=products[i].product_url%>">
                                                    <input type="hidden" name="shoe_item-<%=products[i]._id%>" value="<%=products[i].name%>">
                                                    <input type="hidden" name="amount-<%=products[i]._id%>" value= "<%=products[i].price.price_value>0?products[i].price.price_value:products[i].price.number_price%>">

<!--                                                    <a href="#" data-toggle="modal" data-target="#myModal1"></a>-->
                                                </form>
                                            <button type="button" class="shoe-cart" id="<%=products[i]._id%>" onclick="addButton(this.id)"><i class="fa fa-cart-plus"   aria-hidden="true"></i></button>

                                            </div>

                                        </div>
                                        <div class="clearfix"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <% } %>
                </div>
                <div class="clearfix"></div>

