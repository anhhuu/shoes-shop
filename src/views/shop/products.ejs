        <!-- product right -->
        <div class="left-ads-display col-md-9">
            <% if(options.numOfPage > 1) { %>
                <div class="row">
                    <div class="pagination">
                        <% if(options.brandChecked) { %>
                            <a href="/products?page=1&brand=<%= options.brandChecked %>">&laquo;</a>
                        <% } else { %>
                            <a href="/products?page=1&brand=<%= options.brandChecked %>">&laquo;</a>
                        <% } %>
                        <% if(parseInt(options.currentPage) - 3 > 1) { %>
                            <a href="#" class="disabled">...</a>
                        <% } %>
                        <% for (let j = parseInt(options.currentPage) - 3; j <= parseInt(options.currentPage) + 3; j++) { %>
                            <% if (j < 1) { %>
                            <% continue;} %>
                            <% if (j > options.numOfPage) { %>
                            <% continue;} %>
                            <% if(j == options.currentPage) { %>
                                <% if(options.brandChecked) { %>
                                    <a href="/products?page=<%= j %>&brand=<%= options.brandChecked %> " class="active">
                                        <%= j %>
                                    </a>
                                <% } else { %>
                                    <a href="/products?page=<%= j %>" class="active">
                                        <%= j %>
                                    </a>
                            <% } } else { %>
                                <% if(options.brandChecked) { %>
                                    <a href="/products?page=<%= j %>&brand=<%= options.brandChecked %>">
                                        <%= j %>
                                    </a>
                                <% } else { %>
                                    <a href="/products?page=<%= j %>">
                                        <%= j %>
                                    </a>
                                <% } %>
                            <% } %>
                        <% } %>
                        <% if(parseInt(options.currentPage) + 3 < options.numOfPage) { %>
                            <a href="#" class="disabled">...</a>
                        <% } %>
                        <% if(options.brandChecked) { %>
                            <a href="/products?page=<%= options.numOfPage %>&brand=<%= options.brandChecked %>">&raquo;</a>
                        <% } else { %>
                            <a href="/products?page=<%= options.numOfPage %>&brand=<%= options.brandChecked %>">&raquo;</a>
                        <% } %>
                    </div>
                </div>
            <% } %>
